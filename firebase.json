{
  "hosting": {
    "public": "public",
    "cleanUrls": true,
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "run":{
          "serviceId": "comuthor-web",
          "region": "asia-southeast1"
        }
      }
    ]
  }
}